{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "private-rb-monitor",
  "main": "src/index.ts",
  "compatibility_date": "2025-09-13",

  "kv_namespaces": [
    { "binding": "USERS_KV", "id": "dev-users-kv" }
  ],

  "env": {
    "production": {
      "vars": {
        "ENVIRONMENT": "production"
      },
      "kv_namespaces": [
        { "binding": "USERS_KV", "id": "USERS_KV_PROD_NAMESPACE_ID" }
      ]
    }
  }
}
